{
  "name": "registry",
  "version": "0.0.0",
  "main": "lib/index.js",
  "author": {
    "name": "Nicolas LAURENT",
    "email": "innercircle@aegypius.com",
    "url": ""
  },
  "license": "MIT",
  "description": "EVE Overseer Registry service",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "xo | tee",
    "test": "npm run lint && nyc --cache --reporter=lcov --reporter=text ava --verbose",
    "build": "babel src -d lib",
    "start": "babel-node index.js"
  },
  "dependencies": {
    "goodly": "^0.3.2",
    "mongorito": "^2.1.2",
    "tv4": "^1.2.7"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.11.0",
    "mockery": "^1.7.0",
    "nyc": "^7.1.0",
    "xo": "^0.16.0"
  },
  "xo": {
    "ignores": [
      "lib/**/*"
    ],
    "esnext": true,
    "space": 2
  },
  "ava": {
    "source": [
      "{src,test}/**/*.{js,json,jsx}",
      "schema/**/*.json"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  },
  "homepage": "",
  "files": [
    "lib"
  ],
  "keywords": []
}
